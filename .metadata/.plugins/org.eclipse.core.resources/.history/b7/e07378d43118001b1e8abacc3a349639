package stepdefs;

public class ElectricityBill {
	
	private int unitsConsumed;
	private double slabUsed;

	public void setUnits(Integer units) {
		// TODO Auto-generated method stub
		unitsConsumed= units;
		
		
	}

	public int getUnits() {
		// TODO Auto-generated method stub
		return unitsConsumed;
	}

	public void setSlab(Double slab) {
		// TODO Auto-generated method stub
		slabUsed= slab;
		
	}
	
	public int getSlabs() {
		// TODO Auto-generated method stub
		return slabUsed;
	}

}
